<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title><%= title %></title>
    <meta name="keywords" content="acute care, johns hopkins, umd, nursing, martha kennedy, np/pa" />
    <meta name="description" content="Acute Care NP/PA Leadership Summit" />
    <%= stylesheet_link_tag 'styles.css' %>
    <%= stylesheet_link_tag 'print.css', :media=>'print' %>
    <%= javascript_include_tag :defaults %>
    <%= csrf_meta_tag %>
    <%- if Rails.env.production? %>
      <%= javascript_include_tag 'google_analytics' %>
    <%- end %>
  </head>
  <body>
    <div id="wrapper">
      <%= render 'layouts/header' %>
      <%= render 'layouts/menu' %>
      <div id="main"><!-- #main content and sidebar area -->
        <div id="main_content">
          <%= yield %>
        </div>
      </div>
      <%= render 'layouts/footer' %>
    </div>
  </body>
</html>

