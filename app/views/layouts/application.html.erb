<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title><%= title %></title>
    <meta name="keywords" content="acute care, johns hopkins, umd, nursing, martha kennedy, np/pa" />
    <meta name="description" content="Acute Care NP/PA Leadership Summit" />
    <%= stylesheet_link_tag 'styles.css' %>
    <%= stylesheet_link_tag 'print.css', :media=>'print' %>
    <%= javascript_include_tag :defaults %>
    <%= csrf_meta_tag %>
    <%- if Rails.env.production? %>
      <script type="text/javascript">
        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-21870147-1']);
        _gaq.push(['_setDomainName', 'none']);
        _gaq.push(['_setAllowLinker', true]);
        _gaq.push(['_trackPageview']);
        (function() {
          var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
          ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
          var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();
      </script>
    <%- end %>
  </head>
  <body>
    <div id="wrapper">
      <%= render 'layouts/header' %>
      <%= render 'layouts/menu' %>
      <div id="main"><!-- #main content and sidebar area -->
        <div id="main_content">
          <%= yield %>
          <%= render 'layouts/side' %> 
        </div>
      </div>
      <%= render 'layouts/footer' %>
    </div>
  </body>
</html>

