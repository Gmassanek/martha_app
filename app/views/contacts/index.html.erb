<div id="container">
  <div id="content">
    <h1>Listing contacts</h1>

    <table>
      <tr>
        <th></th>
        <th>Name</th>
        <th>Email</th>
        <th>Message</th>
        <th>Created</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>

    <% @contacts.each do |contact| %>
      <tr align="left">
        <td><%= index+1 %></td>
        <td><%= contact.name %></td>
        <td><%= contact.email %></td>
        <td><%= contact.message %></td>
        <td>
            <%= reg.created_at.strftime '%m/%d/%Y' %> <br/>
        </td>
        <td><%= link_to 'Show', :controller => "contacts", :action => "show", :id => contact.id %></td>
        <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
      </tr>
    <% end %>
    </table>

    <br />

    <%= link_to 'New Contact', new_contact_path %>
  </div>
</div>
<%= render 'admin/side' %>
